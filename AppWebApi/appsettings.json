    "Logging": {
        //All Logger providers
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Warning"

        },
        "Console": {
            //Valid only for the console
            "LogLevel": {
                "Services": "Warning",
                "AppWebApi.Controllers": "Warning",
                "DbRepos": "Warning"

            }
        },
        "InMemory": {
            //Valid only for the InMemory provider
            "LogLevel": {
                "Services": "Warning",
                "AppWebApi.Controllers": "Warning",
                "DbRepos": "Warning"

            }
        }
    },

    "AllowedHosts": "*",

    "ApplicationSecrets": {
        "SecretStorage": "UserSecrets"
    },

    "DatabaseConnections": {
        "DefaultDataUser": "root", //"gstusr" | "usr" | "supusr" | "dbo" | "root"
        "MigrationUser": "root", //migration is done by root user
        "UseDataSetWithTag": "sql-attractions.sqlserver.docker" //"sql-attractions.sqlserver.docker" | "sql-attractions.mysql.docker" | "sql-attractions.postgresql.docker"
    }
}










